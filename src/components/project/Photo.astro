---
import { Picture } from '@astrojs/image/components'

const image = await import('~/assets/rauris.png')
---

<div class="card h-fit w-full overflow-hidden !bg-neutral-900">
  <Picture
    src={image.default.src}
    alt="Picture of a forest in Rauris, Austria."
    widths={[512]}
    sizes="512px"
    aspectRatio={1}
    quality={85}
    format={['avif', 'webp']}
    loading="lazy"
    class="m-0 h-full w-full object-cover"
  />
</div>
