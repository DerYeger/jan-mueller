{
  "name": "jan-mueller",
  "version": "1.14.9",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:es": "eslint --ext \".js,.vue,.ts\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css,ts}\" --ignore-path .gitignore",
    "lint": "yarn lint:es && yarn lint:style",
    "test": "jest",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,yml}\""
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "1.14.0",
    "@nuxtjs/composition-api": "0.24.4",
    "@nuxtjs/feed": "1.1.0",
    "@nuxtjs/markdownit": "2.0.0",
    "@nuxtjs/sitemap": "2.4.0",
    "core-js": "3.14.0",
    "markdown-it": "12.0.6",
    "markdown-it-attrs": "4.0.0",
    "markdown-it-div": "1.1.0",
    "nuxt": "2.15.6",
    "nuxt-i18n": "6.27.0",
    "prism-themes": "1.7.0",
    "reading-time": "1.3.0",
    "vue-masonry-wall": "0.3.2",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@nuxt/types": "2.15.6",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "1.12.1",
    "@vue/test-utils": "1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.2",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.11.1",
    "eslint-plugin-vuetify": "1.0.1",
    "husky": "4.3.8",
    "jest": "27.0.4",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.19.0",
    "ts-jest": "27.0.3",
    "vue-jest": "3.0.7",
    "vuetify": "2.5.3"
  }
}
