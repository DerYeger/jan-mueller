{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "baseUrl": "./",
    "paths": {
      "~/*": ["src/*"]
    },
    "types": ["preact/jsx-runtime", "vite/client"],
    "skipLibCheck": true
  },
  "vueCompilerOptions": {
    "strictCssModules": true,
    "strictTemplates": true
  }
}
